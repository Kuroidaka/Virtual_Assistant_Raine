generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Task {
  id            String   @id
  title         String
  period_time   String?
  specific_time String?
  repeat        Boolean
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
